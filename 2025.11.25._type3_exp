library(dplyr)
df = read.csv("data/bcc.csv")
rdata <- df


# 사용자 코딩

#str(rdata)
#print(colSums(is.na(rdata)))

g1 <- rdata %>% filter(Classification==1) %>%
mutate(logR=log(Resistin)) %>% pull(logR)

g2 <- rdata %>% filter(Classification==2) %>%
mutate(logR=log(Resistin)) %>% pull(logR)

var1 <- var(g1)
var2 <- var(g2)

f_stat <- ifelse(var1>var2, var1/var2, var2/var1)
#print(round(f_stat,3))
#1.348

t_result <- t.test(g1, g2, var.equal=TRUE)
#print(round(t_result$p.value,3))
#0.003
